key_uid = "axelar"
party_uid = "ampd"
url = "http://127.0.0.1:{{ amplifier.tofnd.ports.default_port }}"

[grpc]
ip_addr = '127.0.0.1'
port = {{ amplifier.ampd.ports.grpc_port }}
global_concurrency_limit = 1024
concurrency_limit_per_connection = 32
request_timeout = '30s'
blockchain_service.chains=[]

